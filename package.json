{
  "name": "vudu",
  "version": "1.4.3",
  "description": "Lightweight CSS in JS solution",
  "main": "dist/vudu.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist --copy-files",
    "start": "webpack-dev-server --hot --progress --colors",
    "test": "ava"
  },
  "author": "Daniel Hunninghake <d@dhunninghake.com> (http://dhunninghake.com/)",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.5.0",
    "json-loader": "^0.5.4",
    "markdown-loader": "^0.1.7",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  },
  "keywords": [
    "react",
    "css",
    "js",
    "css-in-js",
    "inline styles"
  ]
}
